                    <CommandText>select a.WkCtrPerfComment, a.Batchid, a.WkCtrID, a.CommUser ,a.CommDate as commentdate,c.workcentredesc, max(transdate) as transactiondate
from leap_wkctrperfcomment a inner join 
(select MAX(commdate) as max_commdate,Batchid, wkctrid from LeaP_wkCtrPerfComment where Excluded = 1 
group by Batchid, wkctrid) b 
on a.Batchid = b.Batchid and a.WkCtrID = b.WkCtrID and a.CommDate = b.max_commdate
inner join LeaP_WorkCentre c on a.wkctrid = c.WorkCentreID
left outer join LeaP_transactions d on c.WorkCentreCompleteTransaction = d.transtype and a.batchid = d.batchidtrim where a.commdate &gt; '01-01-2013'
group by  a.WkCtrPerfComment, a.Batchid, a.WkCtrID, a.CommUser ,a.CommDate,c.workcentredesc
order by a.Batchid, a.WkCtrID


</CommandText>
