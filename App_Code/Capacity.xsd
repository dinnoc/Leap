                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT SUM(c.avgDuration) AS totalduration, d.WorkCentreDesc, COUNT(c.BatchTrimmed) AS BatchCount
FROM  (SELECT b.BatchTrimmed, a.WorkCentreID, MIN(a.EndDate) AS earliestStart, AVG(a.ProcessDuration) AS avgDuration
               FROM   LeaP_RouteData AS a INNER JOIN
