                    <CommandText>select year(c.earlieststart) as yearES,MONTH(c.earlieststart) as monthES ,sum(c.avgduration) as totalduration, d.workcentredesc, COUNT(c.BatchTrimmed) as numberofbatches from 
	(select B.BatchTrimmed , a.WorkCentreID , min(a.EndDate) as earliestStart , avg(a.processduration) as avgDuration 
	from LeaP_RouteData  a inner join LeaP_spcBatches b on a.spcBatchID =b.spcBatchID 
